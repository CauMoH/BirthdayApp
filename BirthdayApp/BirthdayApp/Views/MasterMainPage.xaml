<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
                  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                  xmlns:prism="http://prismlibrary.com"
                  xmlns:mvvm="clr-namespace:Prism.Mvvm;assembly=Prism.Forms"
                  mvvm:ViewModelLocator.AutowireViewModel="True"
                  x:Class="BirthdayApp.Views.MasterMainPage">

    <MasterDetailPage.Master>
        <NavigationPage Title="Menu">
            <x:Arguments>
                <ContentPage Title="Меню">
                    <StackLayout Padding="20">
                        <Button Text="Авторизация" Command="{Binding NavigateToAuthorizationPageCommand}" />
                        <Button Text="Главная" Command="{Binding NavigateToMainPageCommand}" />
                    </StackLayout>
                </ContentPage>
            </x:Arguments>
            </NavigationPage>
                    
    </MasterDetailPage.Master>
    
</MasterDetailPage>